#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
   
 int n, i, num;
 int pos = 0, neg = 0;
 scanf("%d", &n); // Read number of elements
 for (i = 0; i < n; i++) {
  scanf("%d", &num);
   if (num > 0)
       pos++;
   else if (num < 0)
    neg++;
 }
  printf("Count of Positive Integer is %.2f\n", (float)pos);
  printf("Count of Negative Integer is %.2f\n", (float)neg);

    return 0;

}


