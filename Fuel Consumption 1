#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
  



    float liters, distance;
    scanf("%f", &liters);
    scanf("%f", &distance);
   if (liters <= 0 || distance <= 0) {
       printf("Invalid Input\n");
       return 0;
 }
    float l_per_100km = (liters / distance) * 100;
    float miles = distance * 0.6214;
    float gallons = liters * 0.2642;
    float mpg = miles / gallons;
    printf("Liters/100KM\n");
    printf("%.2f\n", l_per_100km);
    printf("Miles/gallons\n");
    printf("%.2f\n", mpg);
   return 0;

}



        


