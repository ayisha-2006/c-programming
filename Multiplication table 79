#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {


    int n;
    scanf("%d", &n);
    if(n < 1 || n > 10) {
       printf("Invalid Input\n");
  } else {
        for(int i = 1; i <= n; i++) {
          printf("%d x %d = %d\n",n, i, n * i);
        }
    }
  return 0;

}
             
         
             
 

