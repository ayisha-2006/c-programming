#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
  
  int a, b, c;
  scanf("%d\n %d\n %d\n", &a, &b, &c);
  if (a > b) {
    if (a > c)
       printf("a is largest then b and c\n");
    else
       printf("c is largest then a and b\n");
     } else {
        if (b > c)
           printf("b is largest then a and c\n");
        else
            printf("c is largest then a and b\n");
  }

    return 0;

}



        


