#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
   float x1, y1, x2, y2;
   float xm, ym;
   scanf("%f\n%f\n%f\n%f", &x1, &y1, &x2, &y2);
   xm = (x1 + x2) / 2.0;
   ym = (y1 + y2) / 2.0;
   printf("Binoy's house is located at (%.1f,%.1f)\n", xm, ym);
   return 0;
}

