#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int a,b;
    char op;
    scanf("%d", &a);
    scanf("%d", &b);
    scanf(" %c", &op); 
    switch (op) {
     case '+':

            printf("Addition of two number is %.1f\n", a + b * 1.0);
            break;

        case '-':
            printf("Subtraction of two number is %.1f\n", a - b * 1.0);
            break;

        case '*':
            printf("Multiplication of two number is %.1f\n", a * b * 1.0);
            break;

        case '/':
          if (b!= 0)
              printf("Division of two number is %.1f\n", a / (b * 1.0));
          else
              printf("Invalid Input\n");
              break;

        case '%':
         if (b!= 0)
            printf("Modulo of two number is %.1f\n", (float)(a % b));
         else
            printf("Invalid Input\n");
            break;

        default:
            printf("Invalid Input\n");

    }

    return 0;

}
    
