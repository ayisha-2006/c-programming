#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int n, sum=0, temp;
    scanf("%d", &n) ;
    if(n<100) {
        printf ("Invalid Input\n") ;
     }else{
        temp=n;
        while(temp>0) {
            sum+=temp%10;
            temp/=10;
            }
        printf("Sum of digit is %d\n",sum) ;
        }
    return 0;
}
